1 VERSIONINFO
 FILEVERSION @GALAXY_VERSION_MAJOR@, @GALAXY_VERSION_MINOR@, @GALAXY_VERSION_PATCH@, 0
 PRODUCTVERSION @GALAXY_VERSION_MAJOR@, @GALAXY_VERSION_MINOR@, @GALAXY_VERSION_PATCH@, 0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904e4"
        BEGIN
            VALUE "CompanyName", "grasmanek94/UniverseLAN"
            VALUE "FileDescription", "UniverseLAN @UVLAN_COMPONENT@ - https://github.com/grasmanek94/UniverseLAN"
            VALUE "FileVersion","@GALAXY_VERSION_MAJOR@, @GALAXY_VERSION_MINOR@, @GALAXY_VERSION_PATCH@, 0"
            VALUE "InternalName", "@PROJECT_NAME@"
            VALUE "LegalCopyright", "UniverseLAN 2022 - @PROJECT_CURRENT_YEAR@"
            VALUE "OriginalFilename", "@UVLAN_OriginalFilename@"
            VALUE "ProductName", "UniverseLAN @UVLAN_COMPONENT@"
            VALUE "ProductVersion","@GALAXY_VERSION_MAJOR@, @GALAXY_VERSION_MINOR@, @GALAXY_VERSION_PATCH@, 0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
